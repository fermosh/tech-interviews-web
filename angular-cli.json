            "../node_modules/uui-framework/uui/bootstrap/js/bootstrap.min.js",
            "../node_modules/uui-framework/uui/js/uui-core.min.js",
            "../node_modules/uui-framework/uui/js/uui-tree-grid.min.js",
            "../node_modules/uui-framework/uui/js/lib/components/jquery.mCustomScrollbar.concat.min.js",
            "../node_modules/uui-framework/uui/js/lib/components/DataTables-1.10.2/js/jquery.dataTables.min.js",
            "../node_modules/uui-framework/uui/js/uui-tooltip.min.js",
            "../node_modules/uui-framework/uui/js/lib/components/tag-it.min.js",
            "../node_modules/uui-framework/uui/js/uui-tagit.min.js",
